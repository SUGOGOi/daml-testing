module TupleExample where

import Daml.Script

import DA.Tuple

tupleTest: Script ()
tupleTest =  script do
    let 
        twoTuple = ("abc",123)
        threeTuple = ("a",456, True)
        nTuple = (1, "hello", True, [3,4])

    debug $ fst twoTuple
    debug $ snd twoTuple

    debug $ fst3 threeTuple
    debug $ snd3 threeTuple
    debug $ thd3 threeTuple

    debug $ nTuple._1
    debug $ nTuple._4
    debug $ nTuple._2
    